<UserControl x:Class="LocalAniHubFront.Views.Components.Recent_SeasonTable"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LocalAniHubFront.Views.Components"
             xmlns:models="clr-namespace:LocalAniHubFront.Models"
             
            
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             ScrollViewer.CanContentScroll="False">

    <!-- d:DataContext="{d:DesignInstance local:Collection_SimpleEntryTimeList,
                         IsDesignTimeCreatable=False}"-->
    <Grid VerticalAlignment="Stretch"
          HorizontalAlignment="Stretch"
          Visibility="Visible">
        <DataGrid
            Visibility="Visible"
            x:Name="UnifiedEntriesDataGrid"
            Margin="0,24,0,24"
            AutoGenerateColumns="False"
            ItemsSource="{Binding UnifiedEntries}">
        </DataGrid>
    </Grid>
</UserControl>
